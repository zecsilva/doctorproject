				if (ckAnimacao.isSelected()){
					int i = 0;
					try{
						i = Integer.parseInt(txtAnimacao.getText());
					} catch (NumberFormatException e){
						i = 0;
					} 
					
					for (int j = 0; j < i; j++) {
						RecursoEstilo recurso = new Animacao();
						RecursosKB recursosKB = new RecursosKB();
						recursosKB.tell(estilos);
						recursosKB.tell(recurso);
						recursosKB.run();
						System.out.println("após Animação " + j + ": \n" + estilos.toString());						
					}
				}
				
				if (ckAutoAvaliacao.isSelected()){
					RecursoEstilo recurso = new AutoAvaliacao();
